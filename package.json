    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "console.js": "^2.0.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "clean-css-cli": "^4.1.10",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.2",
    "query-string": "^5.1.0",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0"
  }
